drop dataverse test if exists;
create dataverse test;
use dataverse test;

write output to nc1:"rttest/similarity_edit-distance_ints.adm";

let $a := [1, 2, 3, 4, 5, 6, 7]
let $b := [1, 3, 4, 5, 7, 8]
let $results :=
[
  edit-distance($a, $b),
  edit-distance($b, $a)  
]
for $i in $results
return $i